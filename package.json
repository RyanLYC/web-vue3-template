{
  "name": "web-vue3-template",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:pre": "vue-tsc --noEmit --skipLibCheck && vite build --mode staging",
    "build:pro": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,vue} --fix",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss} --fix",
    "prettier:format": "prettier --config .prettierrc.cjs src/**/*.{js,jsx,ts,tsx,vue} --write",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@spiriit/vite-plugin-svg-spritemap": "^1.0.2",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "4.8.3",
    "vite": "^4.3.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^3.0.8",
    "vue-tsc": "^1.6.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint",
      "yarn prettier:format"
    ],
    "*.{html,css,sass,scss,vue}": [
      "yarn lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/.cz-config.cjs"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "driver.js": "^1.1.0",
    "element-plus": "^2.3.8",
    "fuse.js": "^6.6.2",
    "pinia": "^2.1.4",
    "pinia-plugin-persist": "^1.0.0",
    "screenfull": "^6.0.2",
    "vue": "^3.3.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "license": "MIT"
}
